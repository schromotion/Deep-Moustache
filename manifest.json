{
  "mainfest_version": 2,
  
  "name": "Deep Moustache",
  "description": "Adds a little moustache to every picture with a person in your browser",
  "version": "0.0.1",
  
  "permissions": [
    "storage",
  ],
  
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png" },
  
  "browser_action": {
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["deepstache.js"]
    }
  ],
